{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "skipLibCheck": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "types": [
      "jasmine"
    ],
    "baseUrl": ".",
    "paths": {
      "@revector/auth-service": [
        "./app/services/auth-service"
      ],
      "@revector/shared": [
        "./app/shared"
      ],
      "@revector/admin-ui": [
        "./app/ui/admin-ui"
      ],
      "@revector/inline-login-form": [
        "./app/ui/inline-login-form"
      ],
      "@revector/sign-in-panel": [
        "./app/ui/sign-in-panel"
      ],
      "@revector/asciidoctor-panel": [
        "./app/ui/asciidoctor-panel"
      ],
      "@revector/drawer": [
        "./app/ui/widgets/drawer"
      ]
    }
  }
}
